# setup environment variables

# PATH variable
export PATH=$HOME/.emacs.d/bin:$HOME/.local/bin:$PATH

# editor
export EDITOR="emacsclient"
export VISUAL="emacsclient"

# default browser
export BROWSER="qutebrowser"

# aliases
alias vi="busybox vi"
alias sx="startx"

# localization
export LANG="en_US.UTF-8"
